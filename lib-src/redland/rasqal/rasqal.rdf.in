	<foaf:homepage rdf:resource="http://purl.org/net/dajobe/"/>
	<foaf:mbox_sha1sum>970987f991961f2553a1bf2574166fa29befbccb</foaf:mbox_sha1sum>
      </foaf:Person>
    </maintainer>

    <release>
