@prefix rs:      <http://jena.hpl.hp.com/2003/03/result-set#> .

[]    a       rs:ResultSet ;
      rs:resultVariable "p" , "v" ;
      rs:size "1" ;
      rs:solution
              [ rs:binding
                        [ rs:value <http://rdf.hp.com/p1> ;
                          rs:variable "p"
                        ] ;
                rs:binding
                        [ rs:value "value-\u00E9" ;
                          rs:variable "v"
                        ]
              ] .
