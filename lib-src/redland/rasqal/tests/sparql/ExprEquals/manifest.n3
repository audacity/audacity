         mf:action
            [ qt:query  <query-eq-graph-5.rq> ;
              qt:data   <data-eq.ttl> ] ;
         mf:result  <result-eq-graph-5.ttl>
      ]


      [  mf:name    "Equality - 2 var - test equals -- graph" ;
         mf:action
            [ qt:query  <query-eq2-graph-1.rq> ;
              qt:data   <data-eq.ttl> ] ;
         mf:result  <result-eq2-graph-1.ttl>
