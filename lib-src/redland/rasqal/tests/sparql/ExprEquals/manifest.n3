      [  mf:name    "Equality 1-1 -- graph" ;
         mf:action
            [ qt:query  <query-eq-graph-1.rq> ;
              qt:data   <data-eq.ttl> ] ;
         mf:result  <result-eq-graph-1.ttl>
      ]
      [  mf:name    "Equality 1-2 -- graph" ;
